#! F: factorial

factorial x = foldl (*) 1 (1..x);

using pure_func; // for the gnm_help function
gnm_info "factorial" = "f", gnm_help "factorial:factorial of a number"
 ["x:number"] "Computes the factorial of @{x}." [] ["=factorial(10)"] [];
