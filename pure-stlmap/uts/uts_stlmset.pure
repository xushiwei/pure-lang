/* uts_stlmset.pure -- tests for stlmset */
//-- --comment c_off --verbose off

//*** Imports *********************************************************

using system, stlmmap, stlvec;
//- ()

//*** Constructors ******************************************************

let sv1 = ::stlvec ["a","b","c","d","e"]; members sv1;
//- ["a","b","c","d","e"]

let sm1 = stlmset ["a","b","c","d","e"]; members sm1;
//- ["a","b","c","d","e"]

let sm1 = stlmset {"a","b","c","d","e"}; members sm1;
//- ["a","b","c","d","e"]

let sm1 = stlmset sv1; members sm1;
//- ["a","b","c","d","e"]

let sm2 = stlmset sm1; members sm2;
//- ["a","b","c","d","e"]

let sm2 = stlmset (sm1,"c","z"); members sm2;
//- ["c","d","e"]

/*** Info ***************************************************************/

let sm1 = stlmset ["a","b","c","d","e"];
//- ()

let sm2 = stlmset ["b","c","d"];
//- ()

let ok, smx, f, l = stl::range_info sm1; ok;
//- 1

smx===sm1, f==stl::begin sm1, l==stl::pastend sm1;
//- 1,1,1

let ok, smx, f, l = stl::range_info (sm1,"b","e"); ok;
//- 1

members (f,l);
//- ["b","c","d"]

#sm1;
//- 5

stl::distance (sm1,"b",stl::smend);
//- 4

stl::empty emptystlmset;
//- 1

stl::count sm1 "c";
//- 1

stl::container_info sm1;
//- 1,(<),[],[],[]

/*** Accessing Ranges as stlvecs or lists ******************************/

let sm1 = stlmset ["a","b","c","d","e"];
//- ()

let sv1 = stlvec sm1; members sv1;
//- ["a","b","c","d","e"]

stl::vector (sm1,"b","d");;
stl::vector (sm1,"b","d");;
//- {"b","c"}

members (sm1,stl::smbeg,stl::smbeg);
//- []

members (sm1,stl::smbeg,stl::smend);
//- ["a","b","c","d","e"]

members (sm1,stl::smend,stl::smbeg);
//- []

members (sm1,stl::smend,stl::smend);
//- []

members (sm1,"0","c");
//- ["a","b"]

members (sm1,"a","d");
//- ["a","b","c"]

members (sm1,"b","d");
//- ["b","c"]

members (sm1,"b",stl::smend);
//- ["b","c","d","e"]

members (sm1,"c","e");
//- ["c","d"]

members (sm1,"d","b");
//- []

keys (sm1,"a",stl::smend);
//- ["a","b","c","d","e"]

keys (sm1,"a","e");
//- ["a","b","c","d"]

keys (sm1,"b","d");
//- ["b","c"]

vals (sm1,"a",stl::smend);
//- ["a","b","c","d","e"]

vals (sm1,"a","e");
//- ["a","b","c","d"]

vals (sm1,"b","d");
//- ["b","c"]

/*** Accessing Ranges using Streams **********************************/

let sm1 = stlmset ["a","b","c","d","e"];
//- ()

head $ stream sm1;
//- "a"

list $ stream sm1;
//- ["a","b","c","d","e"]

list $ take 3 (stream sm1);
//- ["a","b","c"]

list $ take 10 (stream sm1);
//- ["a","b","c","d","e"]

list $ take 10 $ drop 2 (stream sm1);
//- ["c","d","e"]

//*** Insert *************************************************************/


let sm1 = stlmset ["a","b","c","d","e"];
//- ()

let sm2 = mkstlmset (>);
//- ()

let i_b, i_e = stl::begin sm1, stl::pastend sm1;
//- ()

// Insert members

insert sm2 ("a".."c");
//- 3

members sm2;
//- ["c","b","a"]

insert sm2 {"c","d","e"};
//- 3

members sm2;
//- ["e","d","c","c","b","a"]

// Insert with hinted position

erase sm2; 
//- 6

catch id $ insert sm2 ("a", i_b); // i_b is for sm1
//- bad_argument

let p1 = insert sm2 ("a"); members sm2;
//- ["a"]

let p2 = insert sm2 ("b", p1); members sm2;
//- ["b","a"]

let p3 = insert sm2 ("c", p2); members sm2;
//- ["c","b","a"]

// Insert from range, list, vector, k=>v

erase sm2; 
//- 3

let i = stl::find sm1 "a"; get i;
//- "a"

let j = stl::find sm1 "c"; get j;
//- "c"

members (i,j);
//- ["a","b"]

insert sm2 (i,j); 
//- 2

members sm2;
//- ["b","a"]

insert sm2 ["c"];
//- 1

members sm2;
//- ["c","b","a"]

insert sm2 {"d"};
//- 1

members sm2;
//- ["d","c","b","a"]

insert sm2 ["e"]; 
//- 1

members sm2;
//- ["e","d","c","b","a"]

let p = insert sm2 ("f"); stl::get_key p, members sm2;
//- "f",["f","e","d","c","b","a"]

catch id $ put i 11;
//- bad_argument

insert sm2 (i,j);
//- 2

insert sm2 ["c"]; 
//- 1

insert sm2 {"d"}; 
//- 1

let p = insert sm2 ("e"); stl::get_key p, members sm2;
//- "e",["f","e","e","d","d","c","c","b","b","a","a"]

//*** erase ***************************************************************/

// Erase element at position

let sm1 = stlmset ["a","b","c","d","e"];
//- ()

let i_begin, i_pastend = stl::begin sm1, stl::pastend sm1;
//- ()

let i_b, i_d = stl::find sm1 "b", stl::find sm1 "d"; 
//- ()

erase (sm1,i_begin); 
//- 1

members sm1;
//- ["b","c","d","e"]

erase (sm1,i_d); members sm1;
//- ["b","c","e"]

erase (sm1,i_b); members sm1;
//- ["c","e"]

// Erase ranges

let sm1 = stlmset ["a","b","c","d","e"];
//- ()

let i_begin, i_pastend = stl::begin sm1, stl::pastend sm1;
//- ()

let i_b, i_d = stl::find sm1 "b", stl::find sm1 "d"; 
//- ()

get i_b;
//- "b"

erase (sm1,i_b,i_d);
//- 2

members sm1;
//- ["a","d","e"]

catch id $ get i_b;
//- bad_argument

erase (sm1,i_d,i_pastend);
//- 2

members sm1;
//- ["a"]

erase (sm1,i_begin,i_pastend);
//- 1

members sm1;
//- []

// Attempt erase with bad iterators

let sm1 = stlmset ["a","b","c","d","e"];
//- ()

let sm2 = stlmset ["a","b","c","d","e"];
//- ()

let i_b, i_d = stl::find sm1 "b", stl::find sm1 "d"; 
//- ()

catch id $ erase (sm2,i_b);
//- bad_argument

catch id $ erase (sm2,i_b,i_d);
//- bad_argument

// Erase members with given key

let sm1 = stlmset ["a","b","c","d","e"];
//- ()

erase (sm1,"0");
//- 0

erase (sm1,"z");
//- 0

erase (sm1,"b");
//- 1

members sm1;
//- ["a","c","d","e"]

erase (sm1,"a"); members sm1;
//- ["c","d","e"]

erase (sm1,"e"); members sm1;
//- ["c","d"]

//*** swap ***************************************************************/

let sm1 = stlmset ["a","b"];
//- ()

let sm2 = stlmset ["c","d","e"];
//- ()

stl::swap sm1 sm2;
//- ()

members sm1;
//- ["c","d","e"]

members sm2;
//- ["a","b"]

//** Equality and Lexicographical Comparison ****************************/

let sm1 = stlmset ["a","b","c","d"];
//- ()

let sm2 = stlmset ["b","c","d"];
//- ()

stl::equal_elms sm1 sm1;
//- 1

stl::equal_elms sm1 sm2;
//- 0

stl::equal_elms sm2 sm1;
//- 0

stl::equal_elms (sm1,"b","e") sm1;
//- 0

stl::equal_elms (sm1,"b","e") sm2;
//- 1

stl::equal_elms (sm1,"b","d") (sm2,stl::smbeg, "d");
//- 1

sm1 == sm1;
//- 1

sm1 == sm2;
//- 0

sm2 == sm1;
//- 0

sm1 ~= sm1;
//- 0

sm1 ~= sm2;
//- 1

sm1 <  sm1;
//- 0

sm1 <  sm2;
//- 1

sm2 <  sm1;
//- 0

sm1 <= sm1;
//- 1

sm1 <= sm2;
//- 1

sm2 <= sm1;
//- 0

sm1 >  sm1;
//- 0

sm1 >  sm2;
//- 0

sm2 >  sm1;
//- 1

sm1 >= sm1;
//- 1

sm1 >= sm2;
//- 0

sm2 >= sm1;
//- 1

(sm1,"b",stl::smend) == sm2; // note tuple overrided issue
//- 0

stl::equal_elms sm1 sm1;
//- 1

stl::equal_elms sm1 sm2;
//- 0

stl::equal_elms (sm1,"b",stl::smend) sm2;
//- 1

stl::equal_elms (sm1,"c",stl::smend) sm2;
//- 0

stl::equal_elms (sm1,"c",stl::smend) (sm2,"c",stl::smend);
//- 1

stl::equal_elms sm2 (sm1,"b",stl::smend);
//- 1

(sm1,"b",stl::smend) < sm2;
//- 0

(sm1,"c",stl::smend) < sm2;
//- 0

(sm1,"c",stl::smend) < (sm2,"c",stl::smend);
//- 0

(sm1,"b",stl::smend) < sm2;
//- 0

(sm1,"c",stl::smend) < sm2;
//- 0

(sm1,"c",stl::smend) < (sm2,"c",stl::smend);
//- 0

//*** STL Set Algorithms *********************************************

let sm1 = stlmset ["a","b","c"];
//- ()

let sm2 = stlmset ["b","c","d"];
//- ()

let sm3 = stlmset ["a","b","c","d"];
//- ()


let sm = stl::add sm1 sm2; members sm;
//- ["a","b","b","c","c","d"]

let sm = stl::add sm3 sm2; members sm;
//- ["a","b","b","c","c","d","d"]

let sm = stl::add sm2 sm3; members sm;
//- ["a","b","b","c","c","d","d"]

let sm = stl::union sm1 sm2; members sm;
//- ["a","b","c","d"]

let sm = stl::union sm3 sm2; members sm;
//- ["a","b","c","d"]

let sm = stl::union sm2 sm3; members sm;
//- ["a","b","c","d"]

let sm = stl::intersection sm1 sm2; members sm;
//- ["b","c"]

let sm = stl::intersection sm3 sm2; members sm;
//- ["b","c","d"]

let sm = stl::intersection sm2 sm3; members sm;
//- ["b","c","d"]

let sm = stl::difference sm1 sm2; members sm;
//- ["a"]

let sm = stl::difference sm3 sm2; members sm;
//- ["a"]

let sm = stl::difference sm2 sm3; members sm;
//- []

let sm = stl::symmetric_difference sm1 sm2; members sm;
//- ["a","d"]

let sm = stl::symmetric_difference sm3 sm2; members sm;
//- ["a"]

let sm = stl::symmetric_difference sm2 sm3; members sm;
//- ["a"]

stl::subset sm1 sm2;
//- 0

stl::subset sm3 sm2;
//- 1

stl::subset sm2 sm3;
//- 0

//*** Mapping *****************************************************

let sm1 = stlmset ["a","b","c","d","e"];
//- ()

let i_begin, i_pastend = stl::begin sm1, stl::pastend sm1;
//- ()

let i_b, i_e = stl::find sm1 "b", stl::find sm1 "e";
//- ()

listmap (\e->e+10) sm1;
//- ["k","l","m","n","o"]

listmap (\e->e+10) (i_begin,i_pastend);
//- ["k","l","m","n","o"]

catch id $ listmap (\e->e+10) (i_pastend,i_begin);
//- bad_argument

listmap (\e->e+10) (i_begin,i_begin);
//- []

listmap (\e->e+10) (i_pastend,i_pastend);
//- []

listmap (\e->e+10) (i_b,i_e);
//- ["l","m","n"]

catch id $ listmap (\e->e+10) (i_e,i_b);
//- bad_argument

listmap (\e->e+10) (i_b,i_pastend)
//- ["l","m","n","o"]

catch id $ listmap (\e->e+10) (i_pastend,i_b)
//- bad_argument

listmap (\e->e+10) sm1;
//- ["k","l","m","n","o"]

catmap (\e->[e+10]) (i_begin,i_pastend);
//- ["k","l","m","n","o"]

catch id $ catmap (\e->[e+10]) (i_pastend,i_begin);
//- bad_argument

catmap (\e->[e+10]) (i_begin,i_begin);
//- []

catmap (\e->[e+10]) (i_pastend,i_pastend);
//- []

catmap (\e->[e+10]) (i_b,i_e);
//- ["l","m","n"]

catch id $ catmap (\e->[e+10]) (i_e,i_b);
//- bad_argument

catmap (\e->[e+10]) (i_b,i_pastend)
//- ["l","m","n","o"]

catch id $ catmap (\e->[e+10]) (i_pastend,i_b)
//- bad_argument

[ e+1 | e = sm1];
//- ["b","c","d","e","f"]

[ e | e = sm1; (ord e) mod 2];
//- ["a","c","e"]

{ e | e = sm1};
//- {"a","b","c","d","e"}

{ {e} |  e = sm1; (ord e) mod 2};
//- {"a","c","e"}

//** foldl, foldl1 **************************************************

let sm1 = stlmset ["a","b","c","d","e"];
//- ()

let i_begin, i_pastend = stl::begin sm1, stl::pastend sm1;
//- ()

let i_b, i_e = stl::find sm1 "b", stl::find sm1 "e";
//- ()

foldl (\acc e -> e:acc) ["x"] (i_begin,i_b);
//- ["a","x"]

foldl (\acc e -> e:acc) ["x"] (i_b,i_pastend);
//- ["e","d","c","b","x"]

catch id $ foldl (\acc e -> e:acc) ["x"] (i_pastend,i_b);
//- bad_argument

foldl1 (\acc e -> e+","+acc) sm1;
//- "e,d,c,b,a"

catch id $ foldl1 (\acc e -> e+","+acc) (i_begin,i_begin);
//- bad_argument

foldl1 (\acc e -> e+","+acc) (i_begin,i_pastend);
//- "e,d,c,b,a"

catch id $ foldl1 (\acc e -> e+","+acc) (i_pastend,i_begin);
//- bad_argument

catch id $ foldl1 (\acc e -> e+","+acc) (i_pastend,i_pastend);
//- bad_argument

catch id $ foldl1 (\acc e -> e+","+acc) (i_b,i_b);
//- bad_argument

foldl1 (\acc e -> e+","+acc) (i_b,i_e);
//- "d,c,b"

catch id $ foldl1 (\acc e -> e+","+acc) (i_e,i_b);
//- bad_argument

catch id $ foldl1 (\acc e -> e+","+acc) (i_e,i_e);
//- bad_argument

catch id $ foldl1 (\acc e -> e+","+acc) (i_b,i_begin);
//- bad_argument

foldl1 (\acc e -> e+","+acc) (i_begin,i_b);
//- "a"

foldl1 (\acc e -> e+","+acc) (i_b,i_pastend);
//- "e,d,c,b"

catch id $ foldl1 (\acc e -> e+","+acc) (i_pastend,i_b);
//- bad_argument

