/* uts_stlmap.pure -- tests for stlmap */
//-- --comment c_off --verbose off

//*** Imports *********************************************************

using system, stlmap, stlvec;
//- ()

//*** Constructors ******************************************************

let sv1 = ::stlvec ["a"=>1,"b"=>2,"c"=>3,"d"=>4,"e"=>5]; members sv1;
//- ["a"=>1,"b"=>2,"c"=>3,"d"=>4,"e"=>5]

let sm1 = stlmap ["a"=>1,"b"=>2,"c"=>3,"d"=>4,"e"=>5]; stl::elms sm1;
//- ["a"=>1,"b"=>2,"c"=>3,"d"=>4,"e"=>5]

let sm1 = stlmap {"a"=>1,"b"=>2,"c"=>3,"d"=>4,"e"=>5}; stl::elms sm1;
//- ["a"=>1,"b"=>2,"c"=>3,"d"=>4,"e"=>5]

let sm1 = stlmap sv1; stl::elms sm1;
//- ["a"=>1,"b"=>2,"c"=>3,"d"=>4,"e"=>5]

let sm2 = stlmap sm1; stl::elms sm2;
//- ["a"=>1,"b"=>2,"c"=>3,"d"=>4,"e"=>5]

let sm2 = stlmap (sm1,"c","z"); stl::elms sm2;
//- ["c"=>3,"d"=>4,"e"=>5]

/*** Info ***************************************************************/

let sm1 = stlmap ["a"=>1,"b"=>2,"c"=>3,"d"=>4,"e"=>5];
//- ()

let sm2 = stlmap ["b"=>2,"c"=>3,"d"=>4];
//- ()

let ok, smx, f, l = stl::range_info sm1; ok;
//- 1

smx===sm1, f==stl::begin sm1, l==stl::pastend sm1;
//- 1,1,1

let ok, smx, f, l = stl::range_info (sm1,"b","e"); ok;
//- 1

stl::elms (f,l);
//- ["b"=>2,"c"=>3,"d"=>4]

#sm1;
//- 5

stl::distance (sm1,"b",stl::smend);
//- 4

stl::empty emptystlmap;
//- 1

stl::count sm1 "c";
//- 1

stl::container_info sm1;
//- 0,(<),[],(<),(==)

/*** Accessing Ranges as stlvecs or lists ******************************/

let sm1 = stlmap ["a"=>1,"b"=>2,"c"=>3,"d"=>4,"e"=>5];
//- ()

let sv1 = stlvec sm1; members sv1;
//- ["a"=>1,"b"=>2,"c"=>3,"d"=>4,"e"=>5]

stl::vector (sm1,"b","d");
stl::vector (sm1,"b","d");
//- {"b"=>2,"c"=>3}

stl::elms (sm1,stl::smbeg,stl::smbeg);
//- []

stl::elms (sm1,stl::smbeg,stl::smend);
//- ["a"=>1,"b"=>2,"c"=>3,"d"=>4,"e"=>5]

stl::elms (sm1,stl::smend,stl::smbeg);
//- []

stl::elms (sm1,stl::smend,stl::smend);
//- []

stl::elms (sm1,"0","c");
//- ["a"=>1,"b"=>2]

stl::elms (sm1,"a","d");
//- ["a"=>1,"b"=>2,"c"=>3]

stl::elms (sm1,"b","d");
//- ["b"=>2,"c"=>3]

stl::elms (sm1,"b",stl::smend);
//- ["b"=>2,"c"=>3,"d"=>4,"e"=>5]

stl::elms (sm1,"c","e");
//- ["c"=>3,"d"=>4]

stl::elms (sm1,"d","b");
//- []

keys (sm1,"a",stl::smend);
//- ["a","b","c","d","e"]

keys (sm1,"a","e");
//- ["a","b","c","d"]

keys (sm1,"b","d");
//- ["b","c"]

vals (sm1,"a",stl::smend);
//- [1,2,3,4,5]

vals (sm1,"a","e");
//- [1,2,3,4]

vals (sm1,"b","d");
//- [2,3]

/*** Accessing Ranges using Streams **********************************/

let sm1 = stlmap ["a"=>1,"b"=>2,"c"=>3,"d"=>4,"e"=>5];
//- ()

head $ stream sm1;
//- "a"=>1

list $ stream sm1;
//- ["a"=>1,"b"=>2,"c"=>3,"d"=>4,"e"=>5]

list $ take 3 (stream sm1);
//- ["a"=>1,"b"=>2,"c"=>3]

list $ take 10 (stream sm1);
//- ["a"=>1,"b"=>2,"c"=>3,"d"=>4,"e"=>5]

list $ take 10 $ drop 2 (stream sm1);
//- ["c"=>3,"d"=>4,"e"=>5]

// list $ take 3 (stl::lazy_keys sm1);
// //- ["a","b","c"]

// list $ take 3 (stl::lazy_vals sm1);
// //- [1,2,3]

//*** Insert *************************************************************/

let sm1 = stlmap ["a"=>1,"b"=>2,"c"=>3,"d"=>4,"e"=>5];
//- ()

let sm2 = mkstlmap ((>),0);
//- ()

let i_b, i_e = stl::begin sm1, stl::pastend sm1;
//- ()

// Insert elms

stl::elms sm2;
//- []

let p, ok = insert sm2 ("a"=>(0..3)); stl::get_key p, ok;
//- "a",1

stl::elms sm2;
//- ["a"=>[0,1,2,3]]

let p, ok = insert sm2 ("b"=>{0,1,2,3}); stl::get_key p, ok;
//- "b",1

stl::elms sm2;
//- ["b"=>{0,1,2,3},"a"=>[0,1,2,3]]

// Insert with hinted position

erase sm2; 
//- 2

catch id $ insert sm2 ("a"=>1, i_b); // i_b is for sm1
//- bad_argument

let p1, ok = insert sm2 ("a"=>1); stl::elms sm2;
//- ["a"=>1]

let p2 = insert sm2 ("b"=>2, p1); stl::elms sm2;
//- ["b"=>2,"a"=>1]

let p3 = insert sm2 ("c"=>3, p2); stl::elms sm2;
//- ["c"=>3,"b"=>2,"a"=>1]

// Insert from range, list, vector, k=>v

erase sm2; 
//- 3

let i = stl::find sm1 "a"; get i;
//- "a"=>1

let j = stl::find sm1 "c"; get j;
//- "c"=>3

stl::elms (i,j);
//- ["a"=>1,"b"=>2]

insert sm2 (i,j); 
//- 2

stl::elms sm2;
//- ["b"=>2,"a"=>1]

insert sm2 ["c"=>3];
//- 1

stl::elms sm2;
//- ["c"=>3,"b"=>2,"a"=>1]

insert sm2 {"d"=>4};
//- 1

stl::elms sm2;
//- ["d"=>4,"c"=>3,"b"=>2,"a"=>1]

insert sm2 ["e"]; 
//- 1

stl::elms sm2;
//- ["e"=>0,"d"=>4,"c"=>3,"b"=>2,"a"=>1]

let p, ok = insert sm2 ("f"=>6); stl::get_key p, ok, stl::elms sm2;
//- "f",1,["f"=>6,"e"=>0,"d"=>4,"c"=>3,"b"=>2,"a"=>1]

// Attempt overwrite of existing values

put i 11; get i; stl::elms (i,j);
//- ["a"=>11,"b"=>2]

insert sm2 (i,j);
//- 0

insert sm2 ["c"=>13]; 
//- 0

insert sm2 {"d"=>14}; 
//- 0

let p, ok = insert sm2 ("e"=>10); stl::get_key p, ok, stl::elms sm2;
//- "e",0,["f"=>6,"e"=>0,"d"=>4,"c"=>3,"b"=>2,"a"=>1]

put p 5; stl::elms sm2;
//- ["f"=>6,"e"=>5,"d"=>4,"c"=>3,"b"=>2,"a"=>1]

//*** erase ***************************************************************/

// Erase element at position

let sm1 = stlmap ["a"=>1,"b"=>2,"c"=>3,"d"=>4,"e"=>5];
//- ()

let i_begin, i_pastend = stl::begin sm1, stl::pastend sm1;
//- ()

let i_b, i_d = stl::find sm1 "b", stl::find sm1 "d"; 
//- ()

erase (sm1,i_begin); 
//- 1

stl::elms sm1;
//- ["b"=>2,"c"=>3,"d"=>4,"e"=>5]

erase (sm1,i_d); stl::elms sm1;
//- ["b"=>2,"c"=>3,"e"=>5]

erase (sm1,i_b); stl::elms sm1;
//- ["c"=>3,"e"=>5]

// Erase ranges

let sm1 = stlmap ["a"=>1,"b"=>2,"c"=>3,"d"=>4,"e"=>5];
//- ()

let i_begin, i_pastend = stl::begin sm1, stl::pastend sm1;
//- ()

let i_b, i_d = stl::find sm1 "b", stl::find sm1 "d"; 
//- ()

get i_b;
//- "b"=>2

erase (sm1,i_b,i_d);
//- 2

stl::elms sm1;
//- ["a"=>1,"d"=>4,"e"=>5]

catch id $ get i_b;
//- bad_argument

erase (sm1,i_d,i_pastend);
//- 2

stl::elms sm1;
//- ["a"=>1]

erase (sm1,i_begin,i_pastend);
//- 1

stl::elms sm1;
//- []

// Attempt erase with bad iterators

let sm1 = stlmap ["a"=>1,"b"=>2,"c"=>3,"d"=>4,"e"=>5];
//- ()

let sm2 = stlmap ["a"=>1,"b"=>2,"c"=>3,"d"=>4,"e"=>5];
//- ()

let i_b, i_d = stl::find sm1 "b", stl::find sm1 "d"; 
//- ()

catch id $ erase (sm2,i_b);
//- bad_argument

catch id $ erase (sm2,i_b,i_d);
//- bad_argument

// Erase elms with given key

let sm1 = stlmap ["a"=>1,"b"=>2,"c"=>3,"d"=>4,"e"=>5];
//- ()

erase (sm1,"0");
//- 0

erase (sm1,"z");
//- 0

erase (sm1,"b");
//- 1

stl::elms sm1;
//- ["a"=>1,"c"=>3,"d"=>4,"e"=>5]

erase (sm1,"a"); stl::elms sm1;
//- ["c"=>3,"d"=>4,"e"=>5]

erase (sm1,"e"); stl::elms sm1;
//- ["c"=>3,"d"=>4]

//*** swap ***************************************************************/

let sm1 = stlmap ["a"=>1,"b"=>2];
//- ()

let sm2 = stlmap ["c"=>3,"d"=>4,"e"=>5];
//- ()

stl::swap sm1 sm2;
//- ()

stl::elms sm1;
//- ["c"=>3,"d"=>4,"e"=>5]

stl::elms sm2;
//- ["a"=>1,"b"=>2]

//** Equality and Lexicographical Comparison ****************************/

let sm1 = stlmap ["a"=>1,"b"=>2,"c"=>3,"d"=>4];
//- ()

let sm2 = stlmap ["b"=>2,"c"=>3,"d"=>4];
//- ()

stl::equal sm1 sm1;
//- 1

stl::equal sm1 sm2;
//- 0

stl::equal sm2 sm1;
//- 0

stl::equal (sm1,"b","e") sm1;
//- 0

stl::equal (sm1,"b","e") sm2;
//- 1

stl::equal (sm1,"b","d") (sm2,stl::smbeg, "d");
//- 1

sm1 == sm1;
//- 1

sm1 == sm2;
//- 0

sm2 == sm1;
//- 0

sm1 ~= sm1;
//- 0

sm1 ~= sm2;
//- 1

sm1 <  sm1;
//- 0

sm1 <  sm2;
//- 1

sm2 <  sm1;
//- 0

sm1 <= sm1;
//- 1

sm1 <= sm2;
//- 1

sm2 <= sm1;
//- 0

sm1 >  sm1;
//- 0

sm1 >  sm2;
//- 0

sm2 >  sm1;
//- 1

sm1 >= sm1;
//- 1

sm1 >= sm2;
//- 0

sm2 >= sm1;
//- 1

(sm1,"b",stl::smend) == sm2; // note tuple overrided issue
//- 0

stl::equal sm1 sm1;
//- 1

stl::equal sm1 sm2;
//- 0

stl::equal (sm1,"b",stl::smend) sm2;
//- 1

stl::equal (sm1,"c",stl::smend) sm2;
//- 0

stl::equal (sm1,"c",stl::smend) (sm2,"c",stl::smend);
//- 1

stl::equal sm2 (sm1,"b",stl::smend);
//- 1

(sm1,"b",stl::smend) < sm2;
//- 0

(sm1,"c",stl::smend) < sm2;
//- 0

(sm1,"c",stl::smend) < (sm2,"c",stl::smend);
//- 0

(sm1,"b",stl::smend) < sm2;
//- 0

(sm1,"c",stl::smend) < sm2;
//- 0

(sm1,"c",stl::smend) < (sm2,"c",stl::smend);
//- 0

//*** STL Set Algorithms *********************************************

let sm1 = stlmap ["a"=>1,"b"=>2,"c"=>3];
//- ()

let sm2 = stlmap ["b"=>2,"c"=>3,"d"=>4];
//- ()

let sm3 = stlmap ["a"=>31,"b"=>32,"c"=>33,"d"=>34];
//- ()

let sm = stl::merge sm1 sm2; stl::elms sm;
//- ["a"=>1,"b"=>2,"c"=>3,"d"=>4]

let sm = stl::merge sm3 sm2; stl::elms sm;
//- ["a"=>31,"b"=>32,"c"=>33,"d"=>34]

let sm = stl::merge sm2 sm3; stl::elms sm;
//- ["a"=>31,"b"=>2,"c"=>3,"d"=>4]

let sm = stl::set_union sm1 sm2; stl::elms sm;
//- ["a"=>1,"b"=>2,"c"=>3,"d"=>4]

let sm = stl::set_union sm3 sm2; stl::elms sm;
//- ["a"=>31,"b"=>32,"c"=>33,"d"=>34]

let sm = stl::set_union sm2 sm3; stl::elms sm;
//- ["a"=>31,"b"=>2,"c"=>3,"d"=>4]

let sm = stl::set_intersection sm1 sm2; stl::elms sm;
//- ["b"=>2,"c"=>3]

let sm = stl::set_intersection sm3 sm2; stl::elms sm;
//- ["b"=>32,"c"=>33,"d"=>34]

let sm = stl::set_intersection sm2 sm3; stl::elms sm;
//- ["b"=>2,"c"=>3,"d"=>4]

let sm = stl::set_difference sm1 sm2; stl::elms sm;
//- ["a"=>1]

let sm = stl::set_difference sm3 sm2; stl::elms sm;
//- ["a"=>31]

let sm = stl::set_difference sm2 sm3; stl::elms sm;
//- []

let sm = stl::set_symmetric_difference sm1 sm2; stl::elms sm;
//- ["a"=>1,"d"=>4]

let sm = stl::set_symmetric_difference sm3 sm2; stl::elms sm;
//- ["a"=>31]

let sm = stl::set_symmetric_difference sm2 sm3; stl::elms sm;
//- ["a"=>31]

stl::set_includes sm1 sm2;
//- 0

stl::set_includes sm3 sm2;
//- 1

stl::set_includes sm2 sm3;
//- 0

//*** Mapping *****************************************************

let sm1 = stlmap ["a"=>1,"b"=>2,"c"=>3,"d"=>4,"e"=>5];
//- ()

let i_begin, i_pastend = stl::begin sm1, stl::pastend sm1;
//- ()

let i_b, i_e = stl::find sm1 "b", stl::find sm1 "e";
//- ()

listmap (\(k=>v)->k+1=>v+10) sm1;
//- ["b"=>11,"c"=>12,"d"=>13,"e"=>14,"f"=>15]

listmap (\(k=>v)->k=>v+10) (i_begin,i_pastend);
//- ["a"=>11,"b"=>12,"c"=>13,"d"=>14,"e"=>15]

catch id $ listmap (\(k=>v)->k=>v+10) (i_pastend,i_begin);
//- bad_argument

listmap (\(k=>v)->k=>v+10) (i_begin,i_begin);
//- []

listmap (\(k=>v)->k=>v+10) (i_pastend,i_pastend);
//- []

listmap (\(k=>v)->k=>v+10) (i_b,i_e);
//- ["b"=>12,"c"=>13,"d"=>14]

catch id $ listmap (\(k=>v)->k=>v+10) (i_e,i_b);
//- bad_argument

listmap (\(k=>v)->k=>v+10) (i_b,i_pastend)
//- ["b"=>12,"c"=>13,"d"=>14,"e"=>15]

catch id $ listmap (\(k=>v)->k=>v+10) (i_pastend,i_b)
//- bad_argument

listmap (\(k=>v)->k+1=>v+10) sm1;
//- ["b"=>11,"c"=>12,"d"=>13,"e"=>14,"f"=>15]

catmap (\(k=>v)->[k+1=>v+10]) (i_begin,i_pastend);
//- ["b"=>11,"c"=>12,"d"=>13,"e"=>14,"f"=>15]

catch id $ catmap (\(k=>v)->[k+1=>v+10]) (i_pastend,i_begin);
//- bad_argument

catmap (\(k=>v)->[k+1=>v+10]) (i_begin,i_begin);
//- []

catmap (\(k=>v)->[k+1=>v+10]) (i_pastend,i_pastend);
//- []

catmap (\(k=>v)->[k+1=>v+10]) (i_b,i_e);
//- ["c"=>12,"d"=>13,"e"=>14]

catch id $ catmap (\(k=>v)->[k+1=>v+10]) (i_e,i_b);
//- bad_argument

catmap (\(k=>v)->[k+1=>v+10]) (i_b,i_pastend)
//- ["c"=>12,"d"=>13,"e"=>14,"f"=>15]

catch id $ catmap (\(k=>v)->[k+1=>v+10]) (i_pastend,i_b)
//- bad_argument

[ k+1 | (k=>y) = sm1];
//- ["b","c","d","e","f"]

[ k=>v | (k=>v) = sm1; v mod 2];
//- ["a"=>1,"c"=>3,"e"=>5]

{ k+1 | (k=>y) = sm1};
//- {"b","c","d","e","f"}

{ {k;v} |  (k=>v) = sm1; v mod 2};
//- {"a","c","e";1,3,5}

//*** Mapping into a Stream ******************************************

let sm1 = stlmap ["a"=>1,"b"=>2,"c"=>3,"d"=>4,"e"=>5];
//- ()

head $ stl::lazy_map (\(k=>v)->(k,2*v)) sm1;
//- "a",2

list $ stl::lazy_map (\(k=>v)->(k,2*v)) sm1;
//- [("a",2),("b",4),("c",6),("d",8),("e",10)]

list $ take 3 $ stl::lazy_map (\(k=>v)->(k,2*v)) sm1;
//- [("a",2),("b",4),("c",6)]

//*** foldl, foldl1 **************************************************

let sm1 = stlmap ["a"=>1,"b"=>2,"c"=>3,"d"=>4,"e"=>5];
//- ()

let i_begin, i_pastend = stl::begin sm1, stl::pastend sm1;
