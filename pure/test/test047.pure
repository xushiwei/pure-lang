
// Records (Pure 0.41+).

let r = {x=>5, y=>12};
recordp r; member r x;

r!y; r!![y,x];

r!z; r!![y,x,z,0];

let r1 = insert r (x=>99); r1;
let r1 = insert r1 (z=>77); r1;
let r1 = delete r1 z;

let r1 = insert r ("foo"=>99); r1;
let r1 = insert r ("foo"=>88); r1;

r1!"foo"; r1!"bar"; delete r1 "foo";
